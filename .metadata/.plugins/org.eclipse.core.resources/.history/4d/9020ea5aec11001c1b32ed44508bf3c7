public abstract class Produto {
private String nome;
private int id;
private float preco;
private int tipo;
//private float taxaDeLucro; 
public Produto(String n, int i, float p, String t) {
	nome = n;
	id = p;
	preco = t;
	tipo = i;
}
public void cadastrar() {
	
}
public void editar() {
	
}
public void deletar() {
	
}
public String getNome() {
	return nome;
}
public void setNome(String nome) {
	this.nome = nome;
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public float getPreco() {
	return preco;
}
public void setPreco(float preco) {
	this.preco = preco;
}
public String getTipo() {
	return tipo;
}
public void setTipo(String tipo) {
	this.tipo = tipo;
}
}
